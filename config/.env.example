# N1Hub local development defaults
# Copy this file to config/.env and update as needed.

# Backend (FastAPI engine)
STORE_BACKEND=postgres
N1HUB_POSTGRES_DSN=postgresql://postgres:postgres@localhost:5432/n1hub
N1HUB_REDIS_URL=redis://localhost:6379/0
N1HUB_LLM_PROVIDER=anthropic
N1HUB_LLM_API_KEY=
N1HUB_LLM_MODEL=claude-3-haiku-20240307
N1HUB_EMBEDDING_MODEL=all-MiniLM-L6-v2
N1HUB_EMBEDDING_DIMENSION=384
N1HUB_RATE_LIMIT_UPLOAD=60
N1HUB_RATE_LIMIT_CHAT=60
N1HUB_RATE_LIMIT_PUBLIC=120
N1HUB_MAX_CONCURRENT_JOBS=10
N1HUB_MAX_PAYLOAD_MB=20
N1HUB_RETENTION_DAYS=7

# Frontend (Next.js app)
NEXT_PUBLIC_API_URL=http://127.0.0.1:8000
NEXT_PUBLIC_SSE_URL=http://127.0.0.1:8000/events/stream
ENGINE_BASE_URL=http://127.0.0.1:8000
